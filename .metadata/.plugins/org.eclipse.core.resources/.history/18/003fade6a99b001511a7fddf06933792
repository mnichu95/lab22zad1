package Zadanie1;

import java.util.AbstractList;
import java.util.List;
import java.util.Scanner;


public class MyList { // list of integers
static Object[] data; // list itself. null values at the end
static int capacity=10; // maximum capacity of the list
static int num; // current size of the list





public MyList() {
	 this.capacity = capacity;
	 data = new Object[capacity]; // null array
	 num = 0;
}

public static void add(Object a) {
	 // add an object a to the end of the list
		data[num] = a;
		 num++;
	
}

public int size() {
	
	return data.length;
}

public static void main(String[] args) {
      add(123);
	System.out.println(data[0]);

	}

}